  <div class="container-fluid inner">
  <table class="tableizer-table" id= "simple">
  <tr class="tableizer-firstrow">
    <% @rols.each do |rol| %>
  <th><%= link_to rol , rol_path(rol) %></th>
    <% i= 5 %>
    <%  while i < 16 %>
    <% ano= 2000 +i %>
    <td>  <%= ano %> </td>
    <% hombre="hombre" %>
    <% mujer="mujer" %>
    <% otro="otro" %>
    <td><%= personas(ano, hombre) %></td>
    <td><%= personas(ano, mujer)  %></td>
    <td><%= personas(ano, otro)  %></td>
    <% i= i+1 %>
    <% end %>
</tr>
<% end %>
</table>
